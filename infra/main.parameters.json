{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "openAiLocation": {
        "value": "${AZURE_OPENAI_LOCATION}"
      },
      "devBoxUsersAadGroupId": {
        "value": "${AZURE_DEVBOX_USERS_AAD_GROUP_ID}"
      },
      "devBoxPrivateEndpointSubnetId": {
        "value": "${AZURE_DEVBOX_PRIVATE_ENDPOINT_SUBNET_ID}"
      }
    }
}
